{
  "name": "nestjs-ml",
  "version": "0.0.2",
  "description": "NestJS Machine Learning helpers.",
  "author": "onwd",
  "license": "MIT",
  "readmeFilename": "README.md",
  "main": "build/index.js",
  "files": [
    "build/**/*",
    "*.md"
  ],
  "scripts": {
    "build": "tsc",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "keywords": [
    "nestjs",
    "ml",
    "machine",
    "learning"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/onwd/nestjs-ml"
  },
  "bugs": "https://github.com/onwd/nestjs-ml/issues",
  "peerDependencies": {
    "@nestjs/common": "^8.0.6"
  },
  "dependencies": {},
  "devDependencies": {
    "@nestjs/common": "^8.0.6",
    "@nestjs/core": "^8.0.6",
    "@nestjs/testing": "^8.0.6",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.7.1",
    "jest": "^27.0.6",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.2.1",
    "tsconfig-paths": "^3.10.1",
    "tslint": "^6.1.3",
    "typescript": "^4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "source",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
